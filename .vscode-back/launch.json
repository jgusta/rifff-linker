  "version": "0.2.0",
  "configurations": [

    {
      "env": {
        "DEPLOYMENT":"DEVELOPMENT",
      },
      "request": "launch",
      "name": "Deno Launch",
      "type": "node",
      "program": "${workspaceFolder}/dev.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/local/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all",
        "--unstable",
        "--allow-read",
        "--allow-env"
      ],
      "attachSimplePort": 9229
    },
    {
      "env": {
        "DEPLOYMENT": "DEVELOPMENT",
      },
      "request": "launch",
      "name": "Deno test crypt",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/local/bin/deno",
      "runtimeArgs": [
        "task",
        "crypt-test
      ],
      "attachSimplePort": 9229
    }
  ]
}